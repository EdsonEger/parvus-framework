{
    "name": "adrianobnu/parvus-framework",
    "description": "Parvus framework - A small but strong framework",
    "keywords": ["php", "framework", "mvc"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php" : ">=5.3",
        "philo/laravel-blade": "2.*",
        "illuminate/database": "*",
        "phpmailer/phpmailer": "*",
        "symfony/http-foundation" : "2.5.*",
        "symfony/console": "2.4.x-dev"
    },
    "autoload": {
        "psr-0": {
            "": "src/"
        },
        "psr-4": {
	        "Parvus\\": "src/",
            "Parvus\\Console": "src/Console"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}